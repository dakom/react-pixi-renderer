{
  "name": "react-pixi-renderer",
  "version": "0.1.3-alpha",
  "description": "React Pixi Renderer",
  "main": "./dist/react-pixi-renderer.production.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "dev": "cross-env BUILD_TYPE=dev node fuse.js",
    "dev:production": "cross-env BUILD_TYPE=dev-production node fuse.js",
    "test:unit": "cross-env BUILD_TYPE=test-unit node fuse.js",
    "test:external": "live-server --mount=/dist:./dist ./src/tests/external-browser",
    "build": "npm run clean && npm run _build:plain && npm run _build:production && npm run _build:browser:production && npm run _build:browser:plain && npm run _build:tsc",
    "_build:plain": "cross-env BUILD_TYPE=build-plain node fuse.js && mv ./dist/react-pixi-renderer.js.map ./dist/react-pixi-renderer.dev.js.map",
    "_build:production": "cross-env BUILD_TYPE=build-production node fuse.js",
    "_build:browser:production": "cross-env BUILD_TYPE=build-browser-production node fuse.js",
    "_build:browser:plain": "cross-env BUILD_TYPE=build-browser-plain node fuse.js && mv ./dist/react-pixi-renderer.js.map ./dist/react-pixi-renderer.browser-dev.js.map",
    "_build:tsc": "rimraf ./dist/tsc",
    "//_build:tsc": "tsc && mv ./dist/tsc/typings ./dist && rimraf ./dist/tsc"
  },
  "author": "David Komer",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "4.0.4",
    "@types/node": "8.0.31",
    "@types/pixi.js": "4.5.4",
    "@types/react": "^16.0.7",
    "chai": "^4.1.2",
    "cross-env": "5.0.5",
    "fbjs": "^0.8.16",
    "fuse-box": "2.3.1-beta.22",
    "fuse-test-runner": "1.0.14",
    "live-server": "^1.2.0",
    "object-assign": "4.1.1",
    "pixi.js": "4.5.6",
    "prop-types": "15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "2.6.2",
    "typescript": "2.5.3",
    "uglify-js": "3.1.3"
  }
}
